<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="SC.BL.Domain" namespace="SC.BL.Domain">
  <class name="TicketResponse" table="ticketResponse">
    
    <id name="Id">
      <generator class="native"/>
    </id>
    
    <property name="Text"/>
    <property name="Date" type="DateTime"/>
    <property name="IsClientResponse"/>
    <many-to-one name="Ticket" column="Ticket_TicketNumber" class="Ticket" cascade="all-delete-orphan" />    
  
  </class>
</hibernate-mapping>
