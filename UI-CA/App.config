<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section
        name="hibernate-configuration"
        type="NHibernate.Cfg.ConfigurationSectionHandler, NHibernate"
        />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <connectionStrings>
    <add name="SC_NHibernate" 
         connectionString="Data Source=.\SQLSERVER2012;
                           Initial Catalog=SC_NHibernate;
                           Integrated Security=True"
         providerName="System.Data.SqlClient" /><!-- TOEVOEGEN connectionstring 'MultipleActiveResultSets=True' (for lazy-loading during AsEnumerable on parent-entities) -->
  </connectionStrings>
